    "apis": {
      "cagis": {
        "name": "CAGIS (Cincinnati Area Geographic Information System)",
        "baseUrl": "https://cagisonline.hamilton-co.org/arcgis/rest/services",
        "endpoints": {
          "zoning": {
            "url": "https://cagisonline.hamilton-co.org/arcgis/rest/services/Countywide_Layers/Zoning/MapServer/4/query",
            "description": "Zoning district data for parcels",
            "format": "GeoJSON",
            "queryParams": {
              "where": "1=1",
              "outFields": "*",
              "f": "geojson"
            },
            "fields": [
              "OBJECTID",
              "ZONING_CODE",
              "ZONING_DESC",
              "PARCEL_ID",
              "ADDRESS",
              "GEOMETRY"
            ]
          },
          "parcels": {
            "url": "https://cagisonline.hamilton-co.org/arcgis/rest/services/Countywide_Layers/AuditorParcelInformation/MapServer",
            "description": "Parcel information from Hamilton County Auditor",
            "format": "GeoJSON",
            "queryParams": {
              "where": "1=1",
              "outFields": "*",
              "f": "geojson"
            },
            "fields": [
              "PARCEL_ID",
              "ADDRESS",
              "OWNER_NAME",
              "LAND_USE",
              "ACREAGE",
              "GEOMETRY"
            ]
          }
        }
      }
    },
